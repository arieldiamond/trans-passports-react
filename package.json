    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "chai-react-element": "^1.0.4",
    "eslint": "^2.7.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mkdirp": "latest",
    "mocha": "^2.4.5",
    "ncp": "latest",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "rimraf": "latest",
    "testdouble": "^1.2.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.9.1",
